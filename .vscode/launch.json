{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
       {
            "name": "(gdb) Launch Mini",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/bin/Linux/SDL2GuiHelperMini.bin",
            "args": ["--v=5"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/bin/Linux/",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(Remote) Launch Mini",
            "type": "gdb",
            "request": "attach",
            "remote": true,
            "target": "192.168.2.90:9999",
            "executable": "${workspaceFolder}/bin/PI/SDL2GuiHelperMini.bin",
            "cwd": "${workspaceFolder}/bin/PI/",
            "gdbpath" : "gdb-multiarch",
            "debugger_args": []
            
        },
        {
            "name": "(gdb) Launch Maxi",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/bin/Linux/SDL2GuiHelperMaxi.bin",
            "args": ["--v=5"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/bin/Linux/",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
    ]
}